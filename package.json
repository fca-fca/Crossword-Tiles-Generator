{
  "name": "Crossword Tiles Generator",
  "version": "1.0.0",
  "description": "A laser cutting layout tool",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --asar --platform=darwin --arch=x64 --icon=icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . word-tiles --overwrite --asar --platform=win32 --arch=ia32 --icon=icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE"
  },
  "author": "Felix Alba",
  "license": "ISC",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-packager": "^17.1.2"
  }
}